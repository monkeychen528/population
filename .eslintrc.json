{
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "airbnb-typescript"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "linebreak-style": [
      "error",
      "unix"
    ],
    "react/require-default-props": "off",
    "react/jsx-props-no-spreading": "off",
    "object-shorthand": "off",
    "react/no-this-in-sfc": "off"
  }
}